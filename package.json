{
  "name": "@saebasol/lily",
  "version": "0.1.0",
  "description": "A TypeScript-based API wrapper for Heliotrope API.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "@types/node": "^20.11.0"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "heliotrope",
    "api",
    "wrapper",
    "typescript",
    "fetch"
  ],
  "author": "saebasol",
  "license": "MIT"
} 